<div id="table">
    <table>
    <thead>
        <tr>
            {% for tableHeading in tableHeadings %}
                <th>{{ tableHeading }}</th>
            {% endfor  %}
        </tr>
    </thead>

    <tbody>
        {% for product in products %}
            <tr>
                <td>{{ product.id }}</td>
                <td>{{ product.address }}</td>
                <td>{{ product.timestamp|date }}</td>
                <td>{{ diff['distances'][loop.index0]|number_format(1, '.', ',')}}</td>
                <td>{{ diff['time'][loop.index0] }}</td>
                <td>{{ diff['speed'][loop.index0] }}</td>
            </tr>
        {% endfor %}
    </tbody>
        <tfoot>
        </tfoot>
    </table>
</div>